<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity7">
<TextView
    android:id="@+id/labelport"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Transport"
    android:layout_marginTop="50dp"
    android:layout_marginStart="50dp"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintEnd_toStartOf="@+id/textnet"/>
<TextView
    android:id="@+id/textnet"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="50dp"
    app:layout_constraintStart_toEndOf="@+id/labelport"
    app:layout_constraintTop_toTopOf="@+id/labelport"/>
<TextView
    android:id="@+id/labelip"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="IPアドレス"
    android:layout_marginTop="50dp"
    app:layout_constraintTop_toBottomOf="@+id/labelport"
    app:layout_constraintStart_toStartOf="@+id/labelport"/>
<TextView
    android:id="@+id/textip"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="50dp"
    app:layout_constraintTop_toBottomOf="@+id/labelip"
    app:layout_constraintStart_toStartOf="@+id/labelip"/>
</androidx.constraintlayout.widget.ConstraintLayout>